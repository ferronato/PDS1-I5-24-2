<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="controller.PrincipalController">
   <children>
      <MenuBar layoutY="8.0" prefHeight="25.0" prefWidth="600.0">
        <menus>
          <Menu fx:id="menuArquivo" mnemonicParsing="false" text="Arquivo">
            <items>
                  <MenuItem fx:id="menuPesquisa" mnemonicParsing="false" onAction="#btnPesquisaClick" text="Pesquisa" />
              <MenuItem fx:id="menuSair" mnemonicParsing="false" onAction="#btnSairClick" text="Sair" />
            </items>
          </Menu>
          <Menu fx:id="menuCalculadora" mnemonicParsing="false" text="Calculadora">
            <items>
              <MenuItem fx:id="menuSomar" mnemonicParsing="false" onAction="#btnSomarClick" text="Somar" />
            </items>
          </Menu>
          <Menu fx:id="menuAjuda" mnemonicParsing="false" text="Ajuda">
            <items>
              <MenuItem fx:id="menuSobre" mnemonicParsing="false" onAction="#btnSobreClick" text="Sobre" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
